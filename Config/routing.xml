<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="storeseo.configuration.default" path="/admin/module/StoreSeo" methods="get">
        <default key="_controller">StoreSeo\Controller\StoreSeoConfigController:defaultAction</default>
    </route>

    <route id="storeseo.configuration.save" path="/admin/module/StoreSeo" methods="post">
        <default key="_controller">StoreSeo\Controller\StoreSeoConfigController:saveAction</default>
    </route>
</routes>
